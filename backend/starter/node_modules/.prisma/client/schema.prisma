generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = "mysql://root:root@localhost:3306/cp317"
}

model Comment {
  idComment Int     @id @default(autoincrement())
  title     String? @db.VarChar(45)
  author    String? @db.VarChar(45)
  comment   String? @db.VarChar(256)
}
